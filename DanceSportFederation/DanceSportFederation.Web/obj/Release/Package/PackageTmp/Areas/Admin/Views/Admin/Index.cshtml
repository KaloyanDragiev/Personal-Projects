@model DanceSportFederation.Models.ViewModels.Admin.AdminPageVm
@{
    ViewBag.Title = "Admin Panel";
}

<h2>Admin Panel</h2>

<div class="row">
    <div class="col-sm-5 col-sm-offset-1">
        <h3>Users</h3>
        @foreach (var user in Model.Users)
        {
            <div>
                <h4>@Html.DisplayFor(vm => user.Name)</h4>
                @Html.ActionLink("Edit", "EditUser", new { id = user.Id }, new { @class = "btn btn-md btn-warning" })
            </div>
        }
    </div>
</div>
